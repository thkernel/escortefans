<p id="notice"><%= notice %></p>

<h1>Service Users</h1>

<table>
  <thead>
    <tr>
      <th>Service</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_users.each do |service_user| %>
      <tr>
        <td><%= service_user.service %></td>
        <td><%= service_user.user %></td>
        <td><%= link_to 'Show', service_user %></td>
        <td><%= link_to 'Edit', edit_service_user_path(service_user) %></td>
        <td><%= link_to 'Destroy', service_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service User', new_service_user_path %>
