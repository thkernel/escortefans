$("ul.errors").html("");
<% if @service_user.errors.any? %>
  <% @service_user.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".service-user").html("<%= escape_javascript(render 'profiles/partials/service') %>");
  $("#user-profile-service-modal").modal("hide");
 
<% end %>